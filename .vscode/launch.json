    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "Python: Train Vision to Reward",
            "type": "python",
            "request": "launch",
            "module": "src.train.vision_to_reward_train",
            "justMyCode": true
        },
        {
            "name": "Python: Convert Old Bags",
            "type": "python",
            "request": "launch",
            "module": "src.utils.bag2log",
            "justMyCode": true
        },
        {
            "type": "cppdbg",
            "request": "launch",
            "name": "Launch braind",
            "program": "${workspaceRoot}/build/braind",
            "cwd": "${workspaceRoot}",
            "args": ["--vision_model", "yolov7-tiny-f6c3863fa45a58b1"],
            "setupCommands": [
                {
                    "description": "ignore SIGUSR2 signal",
                    "text": "handle SIGUSR2 nostop noprint pass"
                }
            ]
        },
        {
            "type": "cppdbg",
            "request": "launch",
            "name": "Launch encoderd",
            "program": "${workspaceRoot}/build/encoderd",
            "cwd": "${workspaceRoot}",
            "setupCommands": [
                {
                    "description": "ignore SIGUSR2 signal",
                    "text": "handle SIGUSR2 nostop noprint pass"
                }
            ]
        },
        {
            "type": "cppdbg",
            "request": "launch",
            "name": "Launch ctest_msgvec",
            "program": "${workspaceRoot}/build/msgvec_tests",
            "cwd": "${workspaceRoot}",
            "setupCommands": [
                {
                    "description": "ignore SIGUSR2 signal",
                    "text": "handle SIGUSR2 nostop noprint pass"
                }
            ]
        }
    ]
